<div class="background">
  <div class="filter-background">
    <div class="title-logo">
      <form class="form-home">
        <div class="searchbar">
            <div class="searchbar-content">
                <div class="title-form">Let's find some work !</div>
               <%= simple_form_for :search, :html => { :class => 'form-inline' }, url:projects_path, method: :get do |f| %>
                 <%= f.input :address, placeholder: 'Wich city ?', class: 'placeholderfont', label: false, :input_html => { :size => 45} %>
                 <%= f.input :job_id, collection: Job.all,label: false, prompt: "Choose your Job " %>
                 <%= f.submit "Search", class: 'btn btn-primary' %>
               <% end %>
             </div>
          </div>
         </div>
       </div>
      </form>
    </div>
  </div>
</div>

